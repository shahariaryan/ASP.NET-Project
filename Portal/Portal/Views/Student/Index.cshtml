@model List<Portal.Models.User>
@{
    ViewBag.Title = "Index";
}

<h2>Students</h2>
<table class="table table-bordered">

    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Department</th>
        <th>CGPA</th>
    

    </tr>
    @foreach (var s in Model)
    {
        <tr>
            <td>@s.uid</td>
            <td>@s.name</td>
            <td>@s.department</td>
            <td>@s.cgpa</td>
           
        </tr>
    }

</table>
